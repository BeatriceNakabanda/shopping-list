n      this.editing = null;\n    }\n  }\n};",{"version":3,"sources":["ItemTable.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAyCA,eAAA;AACA,EAAA,IAAA,EAAA,YADA;AAEA;AACA,EAAA,KAAA,EAAA;AACA,IAAA,KAAA,EAAA;AADA,GAHA;AAMA,EAAA,IANA,kBAMA;AACA,WAAA;AACA,MAAA,OAAA,EAAA;AADA,KAAA;AAGA,GAVA;AAWA,EAAA,OAAA,EAAA;AACA;;;AAGA,IAAA,QAJA,oBAIA,EAJA,EAIA;AACA,WAAA,OAAA,GAAA,EAAA;AACA,KANA;AAOA,IAAA,QAPA,oBAOA,IAPA,EAOA;AACA,UAAA,IAAA,CAAA,IAAA,KAAA,EAAA,IAAA,IAAA,KAAA,EAAA,EAAA;AACA,WAAA,KAAA,CAAA,WAAA,EAAA,IAAA,CAAA,EAAA,EAAA,IAAA;AACA,WAAA,OAAA,GAAA,IAAA;AACA;AAXA;AAXA,CAAA","sourcesContent":["<template>\n    <div id=\"item-table\">\n        <p v-if=\"items.length < 1\" class=\"empty-table\"> \n            No Items\n        </p>\n        <!-- Creating a table that displays items added through the form,\n        the items in the table can be edited and deleted -->\n        <table class=\"table table-bordered table-striped\" v-else>\n            <thead class=\"thead-dark\">\n                <tr>\n                    <th>No</th>\n                    <th>Item</th>\n                    <th>Quantity</th>\n                    <th>Actions</th>\n                </tr>\n            </thead>\n            <tbody>\n                <tr v-for=\"item in items\" :key=\"item.id\">\n                    <td>{{ item.id }}</td>\n                    <td v-if= \"editing === item.id\" >\n                        <input type=\"text\" v-model=\"item.name\">\n                    </td>\n                    <td v-else>{{ item.name }}</td>\n                    <td v-if= \"editing === item.id\">\n                        <input type=\"text\" v-model=\"item.quantity\">\n                    </td>\n                    <td v-else >{{ item.quantity }}</td>\n                    <td v-if=\"editing === item.id\">\n                        <button @click=\"editItem(item)\">Save</button>\n                        <button class=\"muted-button\" @click=\"editing = null\">Cancel</button>\n                    </td>\n                    <td v-else>\n                        <button @click=\"editMode(item.id)\">Edit</button> \n                        <button @click=\"$emit('delete:item', item.id)\">Delete</button>\n                    </td>\n                </tr>\n            </tbody>\n        </table>\n    </div>\n</template>\n<script>\nexport default {\n    name: 'item-table',\n    //telling the component that it will recieve data inform of an array\n    props: {\n        items: Array,\n    },\n    data() {\n        return {\n            editing: null,\n        }\n    },\n    methods: {\n        /* creating an editing state that gets the id of the row currently being edited\n        when edit mode is enabled\n        */\n        editMode(id){\n            this.editing = id\n        },\n        editItem(item) {\n            if (item.name ===  '' || item ===  '') return\n            this.$emit('edit:item', item.id, item)\n            this.editing = null\n        },\n        \n\n    }\n}\n</script>\n<style>\n/* #item-table {\npadding-left: 300px;\npadding-right: 300px;\npadding-bottom: 65px;\n} */\n</style>\n"],"sourceRoot":"src/components"}]}